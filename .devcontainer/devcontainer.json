{
    "name": "Python-Selenium-Scraper",
    "dockerFile": "Dockerfile",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-azuretools.vscode-docker"
            ]
        },
        "settings": {
            "python.defaultInterpreterPath": "/usr/local/bin/python"
        }
    },
    "postCreateCommand": "pip install -r requirements.txt"
}
